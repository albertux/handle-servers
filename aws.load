# Access and Secrey Keys
export AWS_ACCESS_KEY=
export AWS_SECRET_KEY=

# EC2, Cloudwatch, RDS and ELB
export EC2_HOME=$HOME/.aws/ec2
export AWS_CLOUDWATCH_HOME=$HOME/.aws/cloudwatch
export AWS_RDS_HOME=$HOME/.aws/rds
export AWS_ELB_HOME=$HOME/.aws/elb

# Credential file.
export AWS_CREDENTIAL_FILE=$HOME/.aws/credential.aws
export PATH=$PATH:$EC2_HOME/bin:$AWS_CLOUDWATCH_HOME/bin:$AWS_RDS_HOME/bin:$AWS_ELB_HOME/bin

# JAVA_HOME
export JAVA_HOME=/usr/lib/jvm/default-java
